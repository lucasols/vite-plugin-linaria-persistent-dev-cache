{
  "packageManager": "pnpm@7.5.0",
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "bench-file-dep-hash": "tsm ./test/benchs.ts",
    "build": "vitest run && pnpm --filter vite-plugin-linaria-persistent-dev-cache build",
    "build:watch": "pnpm --filter vite-plugin-linaria-persistent-dev-cache build:watch",
    "npm-publish": "scripts/check-if-is-sync.sh && pnpm run build && pnpm --filter vite-plugin-linaria-persistent-dev-cache npm-publish"
  },
  "devDependencies": {
    "vite": "^2.9.7",
    "vitest": "0.19.1",
    "dequal": "^2.0.3",
    "@types/node": "^18.6.3",
    "@vitest/ui": "0.19.1",
    "v8-profiler-next": "^1.9.0",
    "tsm": "^2.2.2",
    "typescript": "^4.7.4"
  }
}
